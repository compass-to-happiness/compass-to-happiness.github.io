# Enforce Unix newlines
LICENSE         text eol=lf
.gitattributes  text eol=lf
.gitignore      text eol=lf

*.css           text eol=lf
*.env           text eol=lf
*.html          text eol=lf
*.js            text eol=lf
*.json          text eol=lf
*.md            text eol=lf
*.scss          text eol=lf
*.ts            text eol=lf
*.tsx           text eol=lf
*.txt           text eol=lf
*.yaml          text eol=lf
